    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.6",
    "postcss": "^8.4.47",
    "prisma": "^5.20.0",
    "storybook": "^8.3.4",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
